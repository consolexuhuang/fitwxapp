/* pages/subPackages_needLoad/compensateCoupon/compensateCoupon.wxss */
page{
  background: #896DFF
}
.compensateCoupon{
  width:686rpx;
  min-height:680rpx;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
  margin: 0 auto;
  padding: 50rpx 0 70rpx;
  box-sizing: border-box;
}
.compensateCoupon_title{
  font-size:30rpx;
  font-family:PingFangSC;
  font-weight:500;
  color:rgba(137,109,255,1);
  line-height:42rpx;
  text-align: center;

}
.compensateCoupon_img{
  width:520rpx;
  height:230rpx;
  /* background:rgba(137,109,255,1); */
  box-shadow:0px 31rpx 50rpx 0px rgba(213,203,255,1);
  margin: 78rpx auto 0;
  display: block;
}
.compensateCoupon_state{
  font-size:26rpx;
  font-family:PingFangSC;
  font-weight:400;
  color:#D4D4D4;
  line-height:37rpx;
  text-align: center;
  margin-top: 40rpx;
}
.compensateCoupon_state2{
  color:rgba(144,121,252,1);
}
.compensateCoupon_btn{
  width:350rpx;
  background:linear-gradient(360deg,rgba(155,133,252,1) 0%,rgba(114,87,251,1) 100%);
  box-shadow:0px 5rpx 0px 0px rgba(132,107,246,1);
  border-radius:41rpx;
  font-size:30rpx;
  font-family:PingFangSC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:80rpx;
  text-align: center;
  margin: 80rpx auto 0;
}
.compensateCoupon_info{
  padding: 20rpx 30rpx 120rpx;
  box-sizing: border-box;
}
.compensateCoupon_infoTitle{
  font-size:30rpx;
  font-family:PingFangSC;
  font-weight:600;
  color:rgba(255,255,255,1);
  line-height:42rpx;
  text-align: left;
  margin-bottom: 14rpx;
  margin-left: -12rpx;
}
.compensateCoupon_infoCont{
  font-size:24rpx;
  font-family:PingFangSC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:42rpx;
}
.icon{
  color: #fff;
  font-size: 54rpx;
  text-align: center;
  position: absolute;
  bottom: 50rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}
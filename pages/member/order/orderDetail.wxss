  width: 650rpx;
  height: 825rpx;
  position: absolute;
  top:50%;
  left:50%;
  z-index: 2;
  transform: translate(-50%,-50%);
  border-radius:16rpx;
  background:#fff;
}
.broadcast .content .bg-left{
position: absolute;
left:0;
top:403rpx;
z-index: 0;
width: 159rpx;
height: 68rpx;
}

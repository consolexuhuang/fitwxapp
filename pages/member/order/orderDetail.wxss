/* pages/member/order/orderDetail.wxss */
page{
  background: #fff;
}
.orderDetail_statusData{
  background: #fff;
  padding:20rpx 0 20rpx 35rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  min-height: 207rpx;
  position: relative;
}
.orderDetail_headImgBox{
  width: 132rpx;
  height: 132rpx;
  border-radius: 50%;
  margin-right: 38rpx;
  position: relative;
}
.orderDetail_headImg{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.orderDetail_headWxTag{
  position: absolute;
  right: 0;
  bottom: 4rpx;
  z-index: 100;
  width: 48rpx;
  height: 48rpx;
}
.orderDetail_statusInfo{
  display: flex;
  flex-direction: column;
}
.orderDetail_status{
  text-align: left;
  align-self: flex-start;
  font-size:32rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:rgba(20,20,20,1);
  line-height:70rpx;
  letter-spacing:1rpx;
}
.orderDetail_wait{
  font-size:28rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(137,111,255,1);
  line-height:60rpx;
  text-align: left;
}
.orderDetail_titleBox{
  align-self: flex-end;
}
.orderDetail_title{
  font-size:32rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(20,20,20,1);
  line-height:45rpx;
  letter-spacing:1rpx;
  text-align: left;
  white-space: nowrap;
  text-overflow:ellipsis;
  overflow: hidden;
}
.orderDetail_time{
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(20,20,20,1);
  line-height:33rpx;
  letter-spacing:1rpx;
}
.timedur{
  color: #896DFF
}
.status-box{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}
.signIn{
  box-sizing: border-box;
  background: #E9E4FC; 
  font-size:22rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(137,111,255,1);
  line-height:36rpx;
  padding:0 12rpx;
  margin-left:12rpx;
  text-align: center;
}
.share-tag-line{
  margin:0 30rpx;
  width: 1rpx;
  height: 30rpx;
  background-color: #896DFF;
}
.share-tag{
  margin-right:30rpx;
}
.orderDetail_content{
  width: 100%;
  padding-left: 45rpx;
  box-sizing: border-box;
  background: #fff;
  border-top: 20rpx solid #fafafa;
  /* margin-top: 20rpx; */
}
.admissionCodeShow{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 40rpx 0;
  box-sizing: border-box;
  margin-left: -45rpx;
}
.admissionCodeShowImg{
  width:206rpx;
  height:206rpx;
}
.admissionCodeShowText{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(120,120,120,1);
  line-height:40rpx;
  letter-spacing:1rpx;
}
.admissionCode{
  width: 100%;
  min-height: 100rpx;
  /* display: flex;
  align-items: center; */
  border-bottom: 1rpx solid #E9E9E9;
  padding: 30rpx 40rpx 30rpx 0;
  box-sizing: border-box;
  position: relative;
  display: flex;
}
.admissionCodeNext::after{
  content: '';
  position: absolute;
  border-bottom: 2rpx solid #896DFF;
  border-right: 2rpx solid #896DFF;
  width: 14rpx;
  height: 14rpx;
  transform: rotate(-45deg);
  right: 34rpx;
  top: 0;
  bottom: 0;
  margin: auto;
}
.admissionCode2{
  width: 100%;
  min-height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #E9E9E9;
  padding: 30rpx 40rpx 30rpx 0;
  box-sizing: border-box;
}
.admissionCode:last-child{
  border: none;
}
.orderDetail_icon2{
  vertical-align: middle;
  flex-grow: 0;
  flex-shrink: 0;
  margin-top: 6rpx;
  font-size: 27rpx;
  color: #787878
}
.orderDetail_icon{
  vertical-align: middle;
  flex-grow: 0;
  flex-shrink: 0;
  /* margin-top: 6rpx; */
  align-self: center;
  font-size: 27rpx;
  color: #787878
}
.iconcardPay2{
  width: 29rpx;
  height: 27rpx;
}
.orderDetail_icon_careful{
  /* width: 29rpx;
  height: 28rpx; */
  /* align-self: flex-start; */
  margin-top: 6rpx;
  flex-grow: 0;
  flex-shrink: 0;
  font-size: 27rpx;
  color: #787878
}
.orderDetail_text{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(120,120,120,1);
  line-height:38rpx;
  letter-spacing:1rpx;
  margin: 0 0 0 20rpx;
  flex-grow: 0;
  flex-shrink: 0;
}
.orderDetail_data{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(120,120,120,1);
  line-height:40rpx;
  letter-spacing:1rpx;
  margin: 0 20rpx;
  text-align: left;
  display: inline;
  padding-right: 10rpx;
}

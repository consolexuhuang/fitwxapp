/* pages/subPackages_report/newUseReport/newUseReport.wxss */
page{
  width: 100%;
  height: 100%;
  background-image: url('https://img.cdn.powerpower.net/5dfb13e5e4b02f008c7702d1.jpg');
  background-size: 100% 100%;
  background-repeat:no-repeat;
  background-attachment: fixed;
}
.item{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: relative;
  overflow: hidden;
}
.item_bg_middle_Lakeside{
  position: absolute;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 600rpx;
}
.item_bg_cloud{
  position: absolute;
  bottom: -40rpx;
  right: 0;
  left: -100rpx;
  margin: auto;
  width: 140%;
  height: 255rpx;
}
.content{
  position: absolute;
  top: 100rpx;
  /* left: 66rpx; */
  text-align: left;
  padding: 0 66rpx;
  box-sizing: border-box;
  width: 100%
}
.logo{
  font-size: 40rpx;
  color: #fff;
  display: inline-block;
  margin-bottom: 100rpx;
}
.commonText{
  font-size: 32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  position: relative;
  /* line-height:55rpx; */
}
.lightText{
  color: #FFC000;
  font-size: 48rpx;
}
.famousWord{
  display:inline-block;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(212,205,255,.5);
  line-height:47rpx;
  /* opacity:0.5; */
  margin-top: 120rpx;
  text-indent: -16rpx;
}
.openBtn{
  width:340rpx;
  background:rgba(250,189,0,1);
  border-radius:40rpx;
  font-size:34rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:80rpx;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 100rpx;
  margin: auto;
  text-align: center 
}
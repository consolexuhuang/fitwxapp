        <view class="loadCont fadeOut animated delay-6s">
            <text>loading</text>
            <text id="loading1">
                <text class="demo1"></text>
                <text class="demo1"></text>
                <text class="demo1"></text>
              </text>
        </view>
        <view class="btn fadeIn animated delay-6s" bindtap="startReport">查看</view>

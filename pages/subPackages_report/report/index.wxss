/* 升 显 */
@keyframes fadeInUp {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
  }

  100% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
/* 降 显 */
@keyframes fadeInDown {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, -100%, 0);
    transform: translate3d(0, -100%, 0);
  }

  100% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
/* 降 消 */
@keyframes fadeOutDown {
  0% {
    opacity: 1;
  }

  100% {
    opacity: 0;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
  }
}
@keyframes FlashLightAnimate{
  0%{

    filter:  brightness(100%);
    /* opacity: 0; */
  } 
  50%{
    filter:  brightness(120%);
    /* opacity: 0; */
  } 
  100%{
    filter:  brightness(100%);
    /* opacity: 1; */
  }
}
@keyframes FlashLightSlowAnimate{
  0%{
    filter:  brightness(40%);
  }
  33%{
    filter:  brightness(60%);
  }
  66%{
    filter:  brightness(80%);
  }
  100%{
    filter:  brightness(100%);
  }
}
/* 遮光板 */
@keyframes lightBg{
  0%{
    transform: translateY(400rpx);
    opacity: 1
  }
  80%{
    /* transform: translateY(200rpx); */
    opacity: 1
  }
  100%{
    transform: translateY(-500rpx);
    opacity: 0
  }
}
@keyframes loadStarIn{
   0%{
     opacity: 1;
     transform: scale(3) translateY(-500rpx)
   }
   50%{
     transform: scale(1)
   }
   80%{
     /* transform: scale(.8); */
     opacity: 1;
   }
   100%{
     opacity: 0;
     transform: scale(.6) translateY(1000rpx)
   }
}
@keyframes loadScaleAnimate{
  0%{
    opacity: 1;
    transform: scale(6);
   
  }
  99%{
    opacity: 1;
  }
  100%{
    opacity: 0;
    transform: scale(1)
  }
}
.countAminate{
  animation: fadeInUp .5s ease;
  animation-fill-mode: both;
}
.contAnimate.delay04{
  animation: fadeInUp 1s ease .4s;
  animation-fill-mode: both;
}
.contAnimate.delay1{
  animation: fadeInUp 1s ease .8s;
  animation-fill-mode: both;
}
.contAnimate.delay12{
  animation: fadeInUp 2s ease 1.2s;
  animation-fill-mode: both;
}
.contAnimateDowm{
  animation: fadeOutDown .5s ease;
  animation-fill-mode: both;
}
.contAnimateOutDowm{
  animation: fadeInDown .5s ease;
  animation-fill-mode: both;
}
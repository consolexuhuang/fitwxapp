}
.banner-item.videoblock{
  position: relative;
}

.banner-item.videoblock .circle{
  position: absolute;
  top:50%;
  left: 50%;
  transform: translate(-50%,-50%);
width: 90rpx;
  height: 90rpx;
  border-radius: 45rpx;
  background-color:rgba(0,0,0,.5);
  display: flex;

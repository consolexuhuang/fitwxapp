}
.banner-item.videoblock .icon{
  font-size: 30rpx;
  color:#fff;
}
.header .btn-group{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  left:50%;
  bottom:54rpx;
  width: 192rpx;
  margin-left: -81rpx;
}
.header .btn-group .btn{
  width:82rpx;
height:36rpx;
line-height:36rpx;
background:rgba(255,255,255,1);
border-radius:20rpx;
opacity:0.6;
font-size:20rpx;
color:#333333;
text-align: center;
}
.header .btn-group .btn.current{
  background:linear-gradient(90deg,rgba(155,133,252,1) 0%,rgba(114,87,251,1) 100%);

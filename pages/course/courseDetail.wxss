.header{
  width: 100%;
  min-height: 570rpx;
  background-color: #FFFFFF;
  padding-bottom: 20rpx;
}
.banner-swiper{
  padding-top: 10rpx;
  height: 330rpx;
}
.banner-item{
  margin: 0 auto;
  width: 690rpx;
  height: 330rpx;
  border-radius: 12rpx;
  overflow: hidden;
}
.banner-item image{
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
}
.course-name{
  margin-top: 20rpx;
  margin-left: 37rpx;
  font-weight:600;
  font-size: 40rpx;
  color: #141414;
}
.course-time{
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between
}
.course-time .icon1{
  margin-left: 34rpx;
  /* width: 32rpx;
  height: 35rpx; */
  font-size: 30rpx;
  color: #aaa;
}
.course-time text{
  margin-left: 18rpx;
  font-size: 26rpx;
  color: #787878;
}
#line_wx{
  width:2rpx;
  height:30rpx;
  background:linear-gradient(#DFD3FF,#896DFF,#ECE7FF);
  /* margin:0 20rpx 0 350rpx; */
  margin-right: 28rpx;
  
}
.icon_wx_shareBigBox{
  display: flex;
  padding: 0 48rpx;
  box-sizing: border-box;
  align-items: center;
}
.icon_wx_shareBox{
  position: relative;
}
#icon_wx{
  color: #73C92E;
  font-size: 30rpx;
}
.icon_wx_share{
  position: absolute;
  left: 0;
  top: 0;
  width: 31rpx;
  height: 31rpx;
  opacity: 0;
}
.course-address{
  margin-top: 19rpx;
  display: flex;
  align-items: center;
  padding-right: 90rpx
}
.course-address .icon1{
  margin-left: 31rpx;
  /* width: 32rpx;
  height: 34.7rpx; */
  font-size: 34rpx;
  color: #aaa;
  align-self: flex-start
}
.course-address text{
  margin-left: 17rpx;
  font-size: 26rpx;
  color: #787878;
}

.module-wrapper{
  margin-top: 20rpx;
  width: 100%;
  background-color: #FFFFFF;
}
.module-title-wrapper{
  position: relative;
  width: 100%;
  height: 97rpx;
}
.module-dot{
  position: absolute;
  top: 30rpx;
  left: 34rpx;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  background-color: #C9BCFF;
  z-index: 1;
}
.module-title{
  position: absolute;
  top: 35rpx;
  left: 49rpx;
  font-weight:500;
  font-size: 30rpx;
  color: #141414;
  z-index: 2;
}
.coach-avatar-wrapper{
  margin-top: 5rpx;
  display: flex;
  align-items: center;
  padding-bottom: 20rpx
}
.coach-avatar-wrapper_more{
  margin-top: 5rpx;
  /* display: flex;
  flex-direction: column; */
}
.coach-avatarOneBox{
  margin-left: 36rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.coach-avatar{
  margin-top: 5rpx;
  /* margin-left: 36rpx; */
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  flex-shrink: 0;
  align-self: center;
  margin-bottom: 10rpx;
}
.coach-avatar_more{
  margin-top: 5rpx;
  margin-bottom: 10rpx;
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}
.coachBox{
  display: flex;
  padding: 0 36rpx;
  box-sizing: border-box;
}
.coach-avatarBox{
  display: flex;
  flex-direction: column;
  text-align: center;
  margin-left: 40rpx;
}
.coach-name-wrapper{
  margin-left: 41rpx;
  display: flex;
  align-items: center;
}
.coach-name-wrapper_more{
  border-top: 1rpx solid #E6E6E6;
  padding-top: 10rpx;
  margin-top: 20rpx;
}
.coach-name{
  font-weight:500;
  font-size: 28rpx;
  color: #5A5A5A;
}
.coach-description{
  margin-top: 12rpx;
  padding-bottom: 27rpx;
  /* width: 531rpx; */
  line-height: 33rpx;
  font-size: 24rpx;
  color: #787878;
  padding-right: 30rpx;
  /* display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden; */
}

.course-data-list{
  margin-top: 20rpx;
  height: 154rpx;
  background-color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
}
.course-data-item{
  width: 247rpx;
  height: 154rpx;
  text-align: center;
}
.course-data-item-title{
  margin-top: 30rpx;
  font-size: 26rpx;
  color: #8C8C8C;
}
.course-data-item-text{
  margin-top: 15rpx;
  font-weight:500;
  font-size: 30rpx;
  color: #3C3C3C;
}
.course-data-line{
  width: 2rpx;
  height: 20rpx;
  background-color: #D2D2D2;
}
.course-description{
  margin-left: 49rpx;
  width: 659rpx;
  line-height: 33rpx;
  font-size: 24rpx;
  color: #787878;
}
.course-column-list{
  padding: 35rpx 35rpx 35rpx 60rpx;
}
.course-column-item{
  margin-top: 20rpx;
  display: flex;
  margin-bottom: 20rpx;
}
.course-column-item-title-wrapper{
  display: flex;
  align-items: center;
}

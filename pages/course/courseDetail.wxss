.ballBox{
  margin-right: 20rpx;
}
.ball{
  width:20rpx;
  height:20rpx;
  background:rgba(201,188,255,1);
  border-radius: 50%;
  margin-top: 4rpx;
  flex-shrink: 0;
}
.line{
  height: 100%;
  width: 2rpx;
  background: #cdcdcd;
  margin: 0 auto
}
.trainingCamp_introduction_itemTitle{
  font-size:24rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(20,20,20,1);
  line-height:33rpx;
  letter-spacing:1rpx;
}
.trainingCamp_introduction_itemCont{
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(120,120,120,1);
  line-height:33rpx;
  letter-spacing:1rpx;
  padding: 10rpx 0;
  display: block;
}

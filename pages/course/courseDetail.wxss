.course-column-arrow{
  margin-top: 16rpx;
  text-align: center;
}
.course-column-arrow image{
  width: 23rpx;
  height: 13rpx;
}
.fitness-step-content{
  position: relative;
  margin-top: 24rpx;
  width: 100%;
  height: 159rpx;
}
.fitness-step-line{
  position: absolute;
  top: 23rpx;
  left: 106rpx;
  width: 534rpx;
  height: 2rpx;
  background-color: #cdcdcd;
  z-index: 1;
}
.fitness-step-list{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  z-index: 2;
}
.fitness-step-item{
  width: 178rpx;
}
.fitness-step-item-number{
  margin: 0 auto;
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  background-color: #896EFF;
  line-height: 48rpx;
  text-align: center;
  font-size: 30rpx;
  color: #FFFFFF;
}
.fitness-step-item-text{
  margin-top: 33rpx;
  text-align: center;
  font-size: 26rpx;
  color: #141414
}
.tips-wrapper{
  margin-top: 8rpx;
  margin-left: 49rpx;
  padding-bottom: 38rpx;
  line-height: 33rpx;
  width: 643rpx;
  font-size: 24rpx;
  color: #787878;
}
.tips-arrow{
  margin-top: 10rpx;
  text-align: center;
}
.tips-arrow image{
  width: 23rpx;
  height: 13rpx;
}
.tips-text{
  margin-top: 27rpx;
  font-size: 24rpx;
  color: #787878;
}
.tips-text text{
  color: #896DFF;
}
.price-wrapper{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  background-color: #FFFFFF;
  box-shadow:0 -2rpx 4rpx 0 rgba(228,228,228,0.5);
  display: flex;
  z-index: 10;
}
.discount-price{
  flex: 1;
  margin-top: 28rpx;
  margin-left: 37rpx;
  line-height: 59rpx;
  font-weight: 600;
  font-size: 42rpx;
  color: #896EFF;
}
.appoint-btn{
  width: 436rpx;
  height: 120rpx;
  background-color: #896DFF;
  line-height: 120rpx;
  text-align: center;
  font-size: 34rpx;
  color: #FFFFFF;
}
.appoint-btnOver{
  background: #DADADA !important;
  color: #8C8C8C !important;
}